<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:background="#17586E"
    android:orientation="vertical"
    android:padding="10sp" >

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <!-- android:background="@drawable/my_shapes_login_screen"> -->

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content" >

            <ImageView
                android:id="@+id/firstLoginCEPUimgTitleIVID"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="1dp"
                android:layout_marginTop="1dp"
                android:src="@drawable/alert1" />

            <TextView
                android:id="@+id/firstLoginCEPUTitleTVID"
                style="@style/alert_titles"
                android:text="Enter Password !" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:background="#000" />

        <TextView
            android:id="@+id/invalidMembershipTVID"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
       android:text="Enter Password for your app to uninstall"
       android:gravity="center_horizontal"
            android:textColor="#FFF"
            android:textSize="15sp"
            android:textStyle="bold" />
      

        <EditText
            android:id="@+id/resumeTitleETID"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_margin="5dp"
            android:background="@drawable/shape_boundary_rect1"
            android:hint="password"
          
            android:maxLength="25"
            android:singleLine="true" >

            <requestFocus />
        </EditText>

        <!--
    <EditText
        android:id="@+id/webAddressETID"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:hint="@string/alert_title_first_time_login_desc_hint2"
        android:layout_margin="5dp"
        android:inputType="textUri"
        android:background="@drawable/shape_boundary_rect1">
    </EditText>
        -->

         <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginBottom="5dp"
            android:layout_marginTop="5dp"
            android:background="#000" /> 

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:gravity="center" >

            <Button
                android:id="@+id/yesBtnID"
                style="@style/alert_ok_yes_buttons"
                android:background="@null"
                android:text="@string/yes_one" />

            <View
                android:id="@+id/lineVID"
                android:layout_width="1dp"
                android:layout_height="40dp"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_marginBottom="5dp"
                android:layout_marginTop="5dp"
                android:background="#000" />

            <Button
                android:id="@+id/btn_cancel"
                style="@style/alert_button_no"
                android:layout_toRightOf="@+id/lineVID"
                android:background="@null"
                android:text="@string/no_one" />
        </RelativeLayout>
    </LinearLayout>

</LinearLayout>